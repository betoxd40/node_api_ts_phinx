{
    "name": "simple_api",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "engines": {
        "node": ">=14.8.0"
    },
    "scripts": {
        "prebuild": "rm -rf dist/*",
        "build": "tsc && cp -rf package.json dist/package.json",
        "prestart": "npm run build",
        "start": "ts-node .",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "migration:run": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
        "migration:rollback": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/body-parser": "^1.19.0",
        "@types/config": "0.0.36",
        "@types/express": "^4.17.7",
        "@typescript-eslint/eslint-plugin": "^3.9.1",
        "@typescript-eslint/parser": "^3.9.1",
        "eslint": "^7.7.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.0.5",
        "ts-node": "^9.0.0",
        "tsc": "^1.20150623.0",
        "typeorm": "^0.2.25",
        "typescript": "^4.0.2"
    },
    "dependencies": {
        "@hapi/boom": "^9.1.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.7",
        "@types/helmet": "0.0.48",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.6.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "class-validator": "^0.12.2",
        "config": "^3.3.1",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "helmet": "^4.1.0",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "pg": "^8.3.2"
    },
    "_moduleAliases": {
        "@models": "dist/database/models",
        "@repo": "dist/api/repo",
        "@routes": "dist/api/routes",
        "@app/types": "dist/types",
        "@constants": "dist/constants",
        "@utils": "dist/utils"
    }
}